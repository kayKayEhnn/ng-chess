{
  "name": "chess",
  "version": "0.0.1",
  "description": "Angular chess app",
  "engines": {
    "node": "8.11.3"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run server:start",
    "install": "cd server && npm install && cd ../client && npm install",
    "postinstall": "npm run client:build",
